{
    "name": "selenium-mocha-digtal-skola",
    "version": "1.0.0",
    "descripton": "selenium webDriver tests using mocha with mochawesome reports",
    "type": "module",
    "scripts": {
        "test": "mocha test/sesi-11/test_sauce11.js --recursive --timeout 60000 --reporter mochawesome",
        "test1": "mocha test/sesi-9/test-sauce.js --recursive --timeout 60000 --reporter mocha-simple-html-reporter --reporter-option output=sesi-9.html",
        "test2": "mocha test/sesi-9/test-sauce.js --recursive --timeout 60000"
    },
    "dependencies": {
        "selenium-webdriver": "^4.34.0"
